{
  "name": "unpak.js",
  "version": "2.0.0-alpha.1",
  "description": "Modern TypeScript library for reading Unreal Engine pak files and assets, inspired by CUE4Parse",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "url": "https://github.com/kuraima47/unpak.js.git",
    "type": "git"
  },
  "keywords": [
    "unreal-engine",
    "pak-files",
    "iostore",
    "ue4",
    "ue5",
    "fortnite",
